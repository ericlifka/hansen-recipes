{
  "name": "B.B.Q. Ribs",
  "ingredients": [
    "1 cups chili sauce",
    "1 Tbsp worcestershire",
    "1/2 cups water",
    "1 Tbsp oil",
    "1/2 tsp salt",
    "2 Tbsp vinegar",
    "minced onion",
    "1/4 tsp tabasco",
    "1/4 cups lemon juice",
    "2 Tbsp brown sugar",
    "1/4 tsp paprika",
    "1/2 cups ketchup",
    "Ribs"
  ],
  "instructions": [
    "Mix all but the ribs together.",
    "Put sauce on ribs.",
    "Bake for 1 1/2 hours at 325."
  ]
}