{
  "name": "Peach Cobbler",
  "ingredients": [
    "Filling:",
    "4 cups peaches",
    "juice of 1 lemon",
    "1/2 cups bread crumbs",
    "1/2 cups sugar",
    "2 drops vanilla",
    "Topping:",
    "3 oz. sugar",
    "2 oz. butter",
    "salt",
    "cinnamon",
    "1/2 tsp vanilla",
    "5 oz flour"
  ],
  "instructions": [
    "Mix together filling ingredients and put in pan.",
    "Putting topping ingredients in food processor and blend.",
    "Put over peaches.",
    "Bake at 375 for 45 minutes."
  ]
}