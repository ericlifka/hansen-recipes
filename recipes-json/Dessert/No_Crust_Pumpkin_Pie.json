{
  "name": "No Crust Pumpkin Pie",
  "ingredients": [
    "15 oz. can pumpkin",
    "12 oz. can evaporated milk",
    "1/2 cups bisquick",
    "2 eggs",
    "3 tsp brown sugar",
    "2/3 cups sugar",
    "1 Tbsp vanilla",
    "2 tsp cinnamon",
    "1/2 tsp allspice, nutmeg",
    "1/4 tsp ginger, cloves",
    "pinch of salt"
  ],
  "instructions": [
    "Spray pan with PAM.",
    "Throw all together and beat thoroughly.",
    "Bake at 400 for 35-40 minutes.",
    "Test with toothpick for doneness."
  ]
}