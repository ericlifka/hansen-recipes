{
  "name": "Chocolate Chip Cookies",
  "ingredients": [
    "1 3/4 cups shortening",
    "1 1/2 cups each sugar and brown sugar",
    "4 eggs",
    "1 tsp vanilla",
    "dash salt",
    "4 1/2 cups flour",
    "2 tsp soda",
    "1 pkg. chocolate chips"
  ],
  "instructions": [
    "Mix.",
    "Bake at 350 for about 10 minutes."
  ]
}