{
  "name": "Cherry Winks",
  "ingredients": [
    "2 1/4 cups flour",
    "2 tsp baking powder",
    "1/2 tsp salt",
    "3/4 cups butter",
    "1 cups sugar",
    "2 eggs",
    "2 Tbsp milk",
    "1 tsp vanilla",
    "1 cups nuts",
    "1 cups finely chopped dates",
    "1/3 cups finely chopped cherries",
    "2 2/3 cups crushed corn flakes",
    "15 cherries cut in quarters"
  ],
  "instructions": [
    "Sift together flour, baking powder and salt.",
    "Set aside.",
    "In bowl mix butter and sugar until light and fluffy.",
    "Add eggs, beat well.",
    "Stir in milk and vanilla.",
    "Add flour.",
    "Mix well.",
    "Stir in nuts, dates and cherries.",
    "Shape tablespoon of dough into balls.",
    "Roll in corn flakes.",
    "Place on cookie sheets.",
    "Top with cherry quarter.",
    "Bake at 350 for 10-14 minutes."
  ]
}