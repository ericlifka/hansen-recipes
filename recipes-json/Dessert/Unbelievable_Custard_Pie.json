{
  "name": "Unbelievable Custard Pie",
  "ingredients": [
    "2 cups milk",
    "3/4 cups sugar",
    "1/4 tsp baking powder",
    "1/2 cups melted butter",
    "4 eggs",
    "1/2 cups flour",
    "1 tsp vanilla",
    "7 oz. pkg. coconut"
  ],
  "instructions": [
    "Measure ingredients into blender container.",
    "Process on low until well mixed.",
    "Pour into 9 in pie pan.",
    "Bake in 350 oven for 40 minutes."
  ]
}