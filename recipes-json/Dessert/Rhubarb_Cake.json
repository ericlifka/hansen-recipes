{
  "name": "Rhubarb Cake",
  "ingredients": [
    "1/2 cups shortening",
    "1 1/2 cups sugar",
    "1 cups sour milk",
    "1 egg",
    "1 tsp vanilla",
    "3 cups chopped rhubarb",
    "2 cups flour",
    "1 tsp soda",
    "1/2 tsp salt"
  ],
  "instructions": [
    "Mix together.",
    "Add rhubarb last.",
    "Mix some  sugar and cinnamon and sprinkle over top.",
    "Bake in 9 x 13 pan at 350 for 45 min."
  ]
}