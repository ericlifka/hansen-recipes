{
  "name": "Hershey’s Chocolate Cake",
  "ingredients": [
    "2 cups sugar",
    "1 3/4 cups flour",
    "3/4 cups hershey’s cocoa",
    "1 1/2 tsp baking powder & soda",
    "1 tsp salt",
    "2 eggs",
    "1 cups milk",
    "1/2 cups veggie oil",
    "2 tsp vanilla",
    "1 cups boiling water"
  ],
  "instructions": [
    "Heat oven to 350.",
    "Combine dry ingredients in bowl.",
    "Add eggs, milk, oil and vanilla.",
    "Beat 2 minutes on medium speed.",
    "Stir in boiling water.",
    "Batter will be thin.",
    "Pour into 2 9” pans.",
    "Bake 30-35 minutes.",
    "Cool and frost"
  ]
}