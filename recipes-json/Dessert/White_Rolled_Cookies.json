{
  "name": "White Rolled Cookies",
  "ingredients": [
    "1 cups shortening",
    "1/2 cups butter",
    "1 cups sugar",
    "1/2 tsp soda",
    "2 eggs",
    "1 tsp vanilla",
    "2 1/4 cups flour",
    "1 tsp cream of tartar"
  ],
  "instructions": [
    "Mix together and chill 1 hour.",
    "Roll out and cut.",
    "Bake at 350 for 10-15 minutes."
  ]
}