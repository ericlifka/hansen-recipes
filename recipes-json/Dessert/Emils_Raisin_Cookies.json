{
  "name": "Emils Raisin Cookies",
  "ingredients": [
    "1 1/2 cups sugar",
    "1/2 cups shortening",
    "2 eggs",
    "1 cups boiled raisins",
    "1 cups raisin water",
    "dash salt",
    "3/4 cups nuts",
    "1 tsp soda",
    "3 cups flour",
    "1 tsp cinnamon and nutmeg"
  ],
  "instructions": [
    "Mix all and bake at 350."
  ]
}