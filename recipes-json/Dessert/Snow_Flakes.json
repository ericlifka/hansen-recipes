{
  "name": "Snow Flakes",
  "ingredients": [
    "1 cups butter",
    "3 oz. cream cheese",
    "1 cups sugar",
    "1 egg yolk",
    "1 tsp vanilla",
    "1/4 tsp cinnamon",
    "1/2 tsp salt",
    "2 1/2 cups flour",
    "1 tsp orange zest"
  ],
  "instructions": [
    "Cream shortening and cheese well.",
    "Add sugar gradually.",
    "Beat in egg yolk, vanilla and orange rind.",
    "Add dry ingredients.",
    "Put through cookie press."
  ]
}