{
  "name": "Salsa",
  "ingredients": [
    "28 oz. tomato",
    "3 cups celery",
    "1 cucumber, peeled, seeded",
    "19 oz pureed chick peas",
    "1 cups cilantro",
    "4 oz. chopped green chilies",
    "1/2 cups green onion",
    "2 garlic",
    "1/2 Tbsp sugar",
    "1 Tbsp vinegar",
    "1 tsp cumin",
    "1 tsp oregano",
    "1 Tbsp hot sauce",
    "2 Tbsp lime juice"
  ],
  "instructions": [
    "Mix together, cover and refrigerate."
  ]
}