{
  "name": "Buttermilk Salad Dressing",
  "ingredients": [
    "1 cups mayo",
    "1 cups buttermilk",
    "1 teaspoon garlic powder",
    "1/2 teaspoon onion powder",
    "1.4 teaspoon dill",
    "1 tablespoon parsley",
    "Pepper to taste"
  ],
  "instructions": [
    "Mix together and refrigerate."
  ]
}