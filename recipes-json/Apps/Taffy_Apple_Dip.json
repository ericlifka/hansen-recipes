{
  "name": "Taffy Apple Dip",
  "ingredients": [
    "8 oz. cream cheese",
    "3/4 cups brown sugar",
    "1/4 cups sugar",
    "1 Tbsp vanilla",
    "Chopped peanuts"
  ],
  "instructions": [
    "Mix cheese, sugars and vanilla.",
    "Chill.",
    "Before serving top with peanuts.",
    "Serve with apple wedges."
  ]
}