{
  "name": "Fritos Hot Bean Dip",
  "ingredients": [
    "15 oz. pinto beans",
    "4 bottled jalapeno slices",
    "1 Tbsp juice from bottle",
    "1/2 tsp salt",
    "1/2 tsp sugar",
    "1/4 tsp onion powder",
    "1/4 tsp paprika",
    "1/8 tsp garlic powder",
    "1/8 tsp cayenne pepper"
  ],
  "instructions": [
    "Mix in processor until smooth.",
    "Cover and chill at least an hour."
  ]
}