{
  "name": "Zucchini Cucumber Soup",
  "ingredients": [
    "4 Tbsp butter",
    "2 cups onion",
    "2 lb. zucchini",
    "2 cucumbers, seeded but with skins on",
    "3 cups chicken broth",
    "1/4 cups fresh dillsalt and pepper",
    "Garnish:",
    "1/2 cups cuke, tomato"
  ],
  "instructions": [
    "Melt butter and add onions.",
    "Cook 10 min.",
    "Add zucchini and cukes.",
    "Cook over medium for 10 min.",
    "Add broth and cook 15 minutes more.",
    "Remove from heat.",
    "Cool.",
    "Process in blender or food processor.",
    "Serve hot or cold."
  ]
}