{
  "name": "Spicy Pumpkin Soup",
  "ingredients": [
    "3 Tbsp butter",
    "3 cups onion",
    "1 1/2 cups pumpkin",
    "1 cups whole milk",
    "2 1/2 cups chicken broth",
    "3/4 tsp red pepper--use less!",
    "1/4 cups sour cream",
    "1/4 cups cocoa milk",
    "1/2 tsp lime juice."
  ],
  "instructions": [
    "Cook butter and onion 10 minutes and then add rest of stuff through red pepper."
  ]
}