{
  "name": "Hamburger Barley Soup",
  "ingredients": [
    "2 lb. ground beef or turkey",
    "16 oz. tomatoes, chopped",
    "6 oz. tomato juice",
    "7 cups water",
    "1/2 lb. string beans",
    "1 green pepper",
    "1 cups celery",
    "1 tsp salt, pepper",
    "2 tsp garlic powder",
    "2 Tbsp light soy sauce",
    "1 bay leaf",
    "1/2 tsp paprika, thyme",
    "1/2 cups pearl barley",
    "2 carrots",
    "1 large potato"
  ],
  "instructions": [
    "Brown hamburger.",
    "Add everything but carrot and potato.",
    "Bring to a boil, reduce heat simmer 1 hour.",
    "Add carrots and cook 10 minutes.",
    "Add potato and cook 20 minutes more.",
    "Can use almost any veggie."
  ]
}