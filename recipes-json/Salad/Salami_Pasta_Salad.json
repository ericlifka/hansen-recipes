{
  "name": "Salami Pasta Salad",
  "ingredients": [
    "2 cups small pasta shells",
    "3/4 cups green pepper and fresh tomato",
    "1/2 cups salami, cube",
    "1/2 cups pepperoni, chopped",
    "1/2 cups quartered whole ripe olives",
    "1/3 cups onion",
    "2 oz provolone cubed",
    "Dressing:",
    "1/3 cups v. oil",
    "1/4 cups red wine vinegar",
    "2 Tbsp sugar",
    "1 1/2 tsp salt and dried oregano",
    "1/2 tsp pepper"
  ],
  "instructions": [
    "Cook pasta and drain.",
    "Add rest of ingredients except dressing to pasta.",
    "In a jar combine dressing ingredients and shake well.",
    "Pour over pasta and mix.",
    "Cover and refrigerate. before serving."
  ]
}