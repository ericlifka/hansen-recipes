{
  "name": "Vegetable Pasta Salad",
  "ingredients": [
    "4 cups tricolor spiral pasta",
    "16 oz. salsa",
    "2 cups thawed frozen corn",
    "2 cups slice halved cuke",
    "1 cups halved cherry tomato",
    "1 med. red onion",
    "8 oz. no salt tomato sauce",
    "2 Tbsp fresh parsley",
    "1 Tbsp red wine vinegar"
  ],
  "instructions": [
    "Cook pasta and drain.",
    "Combine pasta with remaining ingredients and refrigerate for 30 minutes before serving."
  ]
}