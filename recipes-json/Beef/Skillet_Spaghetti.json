{
  "name": "Skillet Spaghetti",
  "ingredients": [
    "1 lb. hamburger",
    "1 tsp garlic",
    "1 can tomato paste",
    "1 tsp chili powder",
    "1 tsp oregano",
    "8 oz. spaghetti",
    "dash pepper",
    "onion",
    "8 oz. tomato sauce",
    "1 1/2 cups water",
    "2 cups tomato juice",
    "2 tsp salt",
    "1 tsp sugar"
  ],
  "instructions": [
    "Combine all ingredients but spaghetti.",
    "Cover and bring to a boil.",
    "Reduce heat and simmer 30 minutes.",
    "Add spaghetti and simmer covered another 30 minutes."
  ]
}