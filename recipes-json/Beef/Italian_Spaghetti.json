{
  "name": "Italian Spaghetti",
  "ingredients": [
    "1 1/4 lb. hamburger",
    "onion",
    "garlic",
    "2 envelopes sauce mix",
    "2 cans tomato sauce",
    "2 cans tomato paste",
    "3 cups water (mix water/red wine)",
    "1 can mushrooms",
    "1 tsp oregano leaves",
    "1 tsp basil leaves",
    "spaghetti"
  ],
  "instructions": [
    "Cook beef, drain.",
    "Add sauce mix and remaining ingredients except spaghetti.",
    "Simmer until thickened about 30 minutes."
  ]
}