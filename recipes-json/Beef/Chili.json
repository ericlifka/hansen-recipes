{
  "name": "Chili",
  "ingredients": [
    "2 Tbsp oil",
    "2 Tbsp chuck steak (cubed)",
    "1 Tbsp flour 1 onion",
    "3/4 cups red wine",
    "1 cups tomato sauce",
    "1 garlic",
    "1 carrot",
    "1 green pepper",
    "2 bay leaves",
    "40 oz. canned beans (different kinds)",
    "1/2 tsp cumin",
    "1 tsp oregano",
    "1/4 tsp crushed red pepper",
    "10 1/2 oz. can minestrone and lentil soup."
  ],
  "instructions": [
    "Heat oil.",
    "Brown steak in oil with flour.",
    "Add rest of ingredients.",
    "Simmer 1 1/2 hour.",
    "Uncover last 30 minutes if needed to thicken."
  ]
}