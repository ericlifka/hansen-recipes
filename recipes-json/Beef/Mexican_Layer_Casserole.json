{
  "name": "Mexican Layer Casserole",
  "ingredients": [
    "1 lb. hamburger",
    "1 onion",
    "1 can refried beans",
    "1 8 oz. can tomato sauce",
    "1 pkg. enchilada sauce mix",
    "1/2 cups water",
    "8 oz. tortilla chips",
    "4 oz. cheese",
    "1/2 cups black olives",
    "2 cups shredded lettuce",
    "1 tomato"
  ],
  "instructions": [
    "Cook beef.",
    "Add onion.",
    "Stir in beans, tomato sauce, sauce mix and water.",
    "Cook 5-10 minutes until hot and  bubbly.",
    "Place crushed chips in 12 x 8 dish.",
    "Spoon hot mix over.",
    "Sprinkle with cheese and olives.",
    "Bake about 20-30 minutes at 350.",
    "Top with lettuce and tomato."
  ]
}