{
  "name": "Spaghetti",
  "ingredients": [
    "2 lb. hamburger",
    "4 32 oz. cans tomato sauce",
    "1 large can tomato paste",
    "1 onion",
    "1 cups dried parsley",
    "1 tsp oregano",
    "2 tsp garlic, basil",
    "salt and pepper"
  ],
  "instructions": [
    "Brown beef, drain.",
    "Add rest of ingredients and bring to a boil.",
    "Simmer for 1 1/2 hour, stirring frequently."
  ]
}