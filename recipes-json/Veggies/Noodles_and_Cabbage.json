{
  "name": "Noodles and Cabbage",
  "ingredients": [
    "1/4 cups butter",
    "1 tsp each sugar, dill weed, and wine vinegar",
    "4 cups shredded cabbage",
    "1/2 cups onion",
    "1/2 tsp salt and pepper",
    "6 oz. cooked egg noodles"
  ],
  "instructions": [
    "In skillet melt butter.",
    "Add onion and cabbage.",
    "Cook, stirring for 5 minutes.",
    "Stir in noodles and spices.",
    "Serve."
  ]
}