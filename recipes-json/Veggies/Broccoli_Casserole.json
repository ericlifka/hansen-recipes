{
  "name": "Broccoli Casserole",
  "ingredients": [
    "20 oz. broccoli or 1-2 bunch fresh (steamed)",
    "8 oz. velveeta",
    "1 onion",
    "1 can cr. of mushroom",
    "1 cups mayo",
    "cracker crumbs or fried onion"
  ],
  "instructions": [
    "Combine all but cracker crumbs.",
    "Bake at 350 for 45 minutes.",
    "Top with crumbs or onion and reheat long enough to toast."
  ]
}