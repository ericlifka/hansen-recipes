{
  "name": "Southwestern Pan Fried Chicken",
  "ingredients": [
    "4 chicken breasts",
    "1 cups Cheddar",
    "1 Tbsp garlic",
    "3 egg whites",
    "2 chopped tomato",
    "2 tsp cilantro",
    "1 chili pepper",
    "4 gr. onion",
    "1/2 cups cream"
  ],
  "instructions": [
    "Process breasts in whites in processor.",
    "Add remaining ingredients.",
    "Shape into patties.",
    "Dip in 2 c. flour, 1 c. milk, and 4 c. bread crumbs.",
    "Brown on both sides.",
    "Cook in oven 7 minutes at 350.",
    "Top with salsa."
  ]
}