{
  "name": "Chicken Zucchini Hotdish",
  "ingredients": [
    "4 oz. uncooked spag. noodles",
    "1 can cr. of mushroom",
    "1 zucchini",
    "1/2 cups onion",
    "1/4 cups parmesan",
    "1 cups mozzarella",
    "1 clove garlic",
    "1/2 cups milk",
    "2 cups cubed cooked chicken"
  ],
  "instructions": [
    "Break noodles in half.",
    "Cook, drain.",
    "Cook onion and garlic.",
    "Blend in soup.",
    "Add zucchini and milk.",
    "Cook until zucchini is tender.",
    "Stir in mozzarella.",
    "Add noodles and chicken.",
    "Put in casserole; add parmesan on top.",
    "Bake 40 min. at 350."
  ]
}