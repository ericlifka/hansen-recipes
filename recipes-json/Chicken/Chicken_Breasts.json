{
  "name": "Chicken Breasts",
  "ingredients": [
    "6 breasts, skinned, deboned if desired",
    "1 can mushroom soup",
    "1/2 cups sherry or wine",
    "1 cups sour cream",
    "1 small can mushrooms with juice"
  ],
  "instructions": [
    "Lay chicken in baking dish.",
    "Mix other ingredients together and pour over chicken.",
    "Sprinkle with paprika.",
    "Bake uncovered 325-350 about 1 1/2  to 2 hours."
  ]
}