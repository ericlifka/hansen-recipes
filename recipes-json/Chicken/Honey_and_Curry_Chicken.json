{
  "name": "Honey and Curry Chicken",
  "ingredients": [
    "1 stick butter",
    "1/2 cups onion",
    "2 garlic",
    "1/4 tsp Dijon mustard",
    "1/2 cups honey",
    "1 tsp salt and curry powder",
    "2 Tbsp chutney",
    "chicken"
  ],
  "instructions": [
    "Melt butter.",
    "Add onion and garlic.",
    "Add rest and stir.",
    "Set aside to cool.",
    "Coat chicken.",
    "Bake at 400 for 1 hour."
  ]
}