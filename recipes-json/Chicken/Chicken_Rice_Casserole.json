{
  "name": "Chicken Rice Casserole",
  "ingredients": [
    "2 Tbsp oil",
    "1 onion",
    "3 cups cooked chicken",
    "2 15 oz. cans green beans, drain and rinse",
    "8 oz. water chestnuts, chopped",
    "4 oz. pimento",
    "1 cr. of celery soup",
    "1/2 cups mayo",
    "1/2 cups sour cream",
    "6 oz. box cooked wild rice",
    "1 cups cheddar cheese"
  ],
  "instructions": [
    "Saute onion in oil",
    "Mix in all other ingredients",
    "Put in greased pan and bake for 20-25 minutes."
  ]
}