{
  "name": "Ranch Crispy Chicken",
  "ingredients": [
    "1 oz. packet ranch salad dressing",
    "1/4 cups corn flake crumbs",
    "6-8 pieces chicken with skin"
  ],
  "instructions": [
    "Combine dressing mix and crumbs in bag.",
    "Shake chicken in bag.",
    "Bake 375 for 50 minutes."
  ]
}