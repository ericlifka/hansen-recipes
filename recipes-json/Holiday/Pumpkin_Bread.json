{
  "name": "Pumpkin Bread",
  "ingredients": [
    "4 eggs",
    "1/2 tsp baking powder",
    "2/3 cups soft butter",
    "2 tsp baking soda",
    "3 cups sugar",
    "1 tsp cinnamon",
    "15 oz. pumpkin",
    "1/2 tsp cloves",
    "3 cups flour",
    "1/2 tsp nutmeg",
    "1/2 tsp salt",
    "1 cups chopped nuts"
  ],
  "instructions": [
    "Combine eggs, butter and sugar, blending well.",
    "Add the rest of the ingredients.",
    "Pour into 2 greased loaf pans.",
    "Bake at 350ยบ for 1 hour.",
    "Turn onto racks and cool."
  ]
}