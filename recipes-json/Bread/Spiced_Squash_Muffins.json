{
  "name": "Spiced Squash Muffins",
  "ingredients": [
    "2 cups flour",
    "1/3 cups brown sugar",
    "2 tsp baking powder",
    "1 tsp cinnamon",
    "1/2 tsp salt",
    "1/4 tsp ginger",
    "1/4 tsp nutmeg",
    "2 eggs",
    "3/4 cups squash",
    "3/4 cups light corn syrup",
    "1/4 cups butter, melted",
    "1/4 cups veggie oil",
    "1 tsp vanilla",
    "Topping:",
    "4 Tbsp butter",
    "1/2 cups brown sugar",
    "1 tsp cinnamon"
  ],
  "instructions": [
    "Combine all dry ingredients and then wet muffin mix stuff.",
    "Fill paper lined muffin cups 3/4 full.",
    "Combine topping mix until crumbly.",
    "Sprinkle on muffins.",
    "Bake 400 for 15-20 minutes.",
    "Cool 5 minutes before putting on wire rack.",
    "Serve warm."
  ]
}