{
  "name": "Southern Living Sweet Potato Biscuits",
  "ingredients": [
    "4 cups flour",
    "2 Tbsp baking powder",
    "2 tsp salt",
    "1 cups butter",
    "3/4-1 cups buttermilk",
    "1 cups sweet potato, mashed"
  ],
  "instructions": [
    "Combine flour, baking powder and salt.",
    "Cut in butter.",
    "Stir together potato and buttermilk.",
    "Turn dough onto floured surface.",
    "Roll to 1/2” thick.",
    "Grease cookie sheet.",
    "Use a 3” biscuit cutter.",
    "Bake at 425 for 10-15 minutes."
  ]
}