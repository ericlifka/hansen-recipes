{
  "name": "Pumpkin Bread",
  "ingredients": [
    "4 eggs",
    "2/3 cups butter",
    "3 cups sugar",
    "15 oz pumpkin",
    "3 cups flour",
    "1/2 tsp salt",
    "1/2 tsp baking powder",
    "2 tsp baking soda",
    "1 tsp cinnamon",
    "1/2 tsp cloves",
    "1/2 tsp nutmeg",
    "1 cups chopped nuts (optional)"
  ],
  "instructions": [
    "Combine eggs, butter and sugar, blending well.",
    "Add the rest of ingredients.",
    "Pour into 2 greased 8 1/2 x 5 1/2 loaf pans and bake at 350 for 1 hour.",
    "Turn onto racks and cool."
  ]
}