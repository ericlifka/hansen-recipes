{
  "name": "Date Muffins",
  "ingredients": [
    "1 cups chopped dates",
    "1 cups boiling water",
    "1 Tbsp shortening",
    "1 egg, beaten",
    "1 tsp vanilla",
    "1 1/2 cups flour",
    "1 cups sugar",
    "1 tsp baking powder",
    "1/2 tsp salt",
    "1 cups pecans"
  ],
  "instructions": [
    "Combine dates, water, and shortening.",
    "Let stand 1 hour.",
    "Stir in egg and vanilla.",
    "Combine flour and rest of ingredients.",
    "Add date mix and stir till moist.",
    "Spoon into muffin pans.",
    "Bake at 350 for 25-30 minutes."
  ]
}