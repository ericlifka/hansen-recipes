{
  "name": "Date Bread",
  "ingredients": [
    "1 cups chopped dates (hand chop)",
    "3/4 cups nuts",
    "1 1/2 tsp baking soda",
    "1/2 tsp salt",
    "1/4 cups butter",
    "3/4 cups boiling water",
    "2 eggs",
    "1 tsp vanilla",
    "1 cups sugar",
    "1 1/2 cups flour"
  ],
  "instructions": [
    "Combine nuts, dates, soda, salt in bowl.",
    "Add butter and water.",
    "Let sit 15 minutes.",
    "Stir.",
    "Beat eggs slightly.",
    "Add vanilla.",
    "Stir in sugar and flour.",
    "Add to date mix.",
    "Do not overmix.",
    "Grease pans bake at 350 for 1 hour.",
    "Makes 2 loaves."
  ]
}