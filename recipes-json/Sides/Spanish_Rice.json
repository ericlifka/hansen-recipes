{
  "name": "Spanish Rice",
  "ingredients": [
    "1 can stewed tomatoes",
    "1 1/2 cups chicken broth",
    "2 tsp chili powder",
    "1 Tbsp butter",
    "1 1/4 cups white rice, uncooked",
    "3/4 tsp oregano",
    "1/2 tsp garlic salt"
  ],
  "instructions": [
    "In medium pan combine all ingredients.",
    "Bring to boil; reduce heat.",
    "Cover and simmer 25 minutes or until rice is done."
  ]
}