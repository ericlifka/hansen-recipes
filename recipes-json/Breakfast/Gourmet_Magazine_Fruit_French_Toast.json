{
  "name": "Gourmet Magazine Fruit French Toast",
  "ingredients": [
    "3 eggs",
    "1 1/2 cups milk",
    "1/2 tsp nutmeg",
    "1 tsp vanilla",
    "1/4 cups brown sugar",
    "1 cups fruit"
  ],
  "instructions": [
    "Mix all but fruit.",
    "Pour over french bread.",
    "Can refrigerate overnight.",
    "Top bread with fruit.",
    "Heat 2 T. butter with 1/4 c. brown sugar.",
    "Pour over bread.",
    "Bake 20 minutes."
  ]
}