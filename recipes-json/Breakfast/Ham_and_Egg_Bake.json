{
  "name": "Ham and Egg Bake",
  "ingredients": [
    "3 cups cooked rice",
    "6 hard boiled eggs, chopped",
    "1 cups diced ham",
    "2 Tbsp mustard",
    "2 Tbsp minced onion",
    "3/4 cups butter",
    "1/2 cups diced cheddar cheese"
  ],
  "instructions": [
    "Combine all ingredients.",
    "Bake at 350 for 30 minutes."
  ]
}