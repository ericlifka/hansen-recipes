{
  "name": "Breakfast Casserole",
  "ingredients": [
    "3 cups frozen hash browns",
    "1 cups diced ham",
    "4 eggs",
    "1/4 tsp pepper, salt",
    "3/4 cups Monterey jack cheese",
    "1/4 cups green onion",
    "12 oz evaporated milk"
  ],
  "instructions": [
    "Grease 2 qt. dish.",
    "Put potatoes on bottom.",
    "Sprinkle with cheese, ham, onion.",
    "Combine rest of ingredients and pour over.",
    "Bake uncovered 350 for 45 min. or until set."
  ]
}